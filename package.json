{
  "name": "voting",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev:client": "vite",
    "dev": "cross-env NODE_ENV=development ts-node --project tsconfig.server.json service/index",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:entry-server": "vite build --ssr src/entry-server.tsx --outDir dist/entry-server",
    "build:server": "tsc --project tsconfig.server.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "hdStart": "hardhat node --tsconfig tsconfig.hat.json",
    "hdCompile": "hardhat compile --tsconfig tsconfig.hat.json",
    "hdTest": "hardhat test --network localhost  --tsconfig tsconfig.hat.json",
    "hdDeploy": "hardhat ignition deploy ./ignition/modules/VotingContract.ts --network localhost",
    "hdDeployLock": "hardhat ignition deploy ./ignition/modules/Lock.ts --network localhost"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.2",
    "@rainbow-me/rainbowkit": "^2.1.6",
    "@tanstack/react-query": "^5.56.2",
    "axios": "^1.7.7",
    "body-parser": "^1.20.3",
    "cos-js-sdk-v5": "^1.8.4",
    "express": "^4.21.0",
    "lodash": "^4.17.21",
    "mysql2": "^3.11.3",
    "react": "^18.3.1",
    "react-countdown": "^2.3.6",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-router-dom": "^6.26.2",
    "sirv": "^2.0.4",
    "viem": "^2.21.10",
    "wagmi": "^2.12.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "hardhat": "^2.22.10",
    "sass-embedded": "^1.79.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  }
}
